{"name":"2d4 250sp Art Objects, Roll once on Magic Item Table F and 1d4 times on Magic Item Table G","type":"script","author":"THB0iiaRQeds6BDT","img":"icons/svg/chest.svg","scope":"global","command":"main()\nasync function main() {\n\n  let r1 = new Roll('2d4').roll({async: false});\n  await console.log(r1.total)\n\n  let r2 = new Roll('1d4').roll({async: false});\n  await console.log(r2.total)\n\n  let r3 = new Roll('1d6').roll({async: false});\n  await console.log(r3.total)\n\n//Reading the pack from Compendium\n\nconst pack = game.packs.get(\"cc5e.rolltables-custom\");\n\nawait pack.getIndex();\n\n//Find the right table\n\nlet entry = pack.index.find(e => e.name === \"250sp Art Objects\");\n\n//Rolling on the table\n\npack.getDocument(entry._id).then(table => table.drawMany(r1.total));\n\n//Find the right table\n\nlet entry2 = pack.index.find(e => e.name === \"Magic Item Table F\");\n\n//Rolling on the table\n\npack.getDocument(entry2._id).then(table => table.drawMany(1));\n\n//Find the right table\n\nlet entry3 = pack.index.find(e => e.name === \"Magic Item Table G\");\n\n//Rolling on the table\n\npack.getDocument(entry3._id).then(table => table.drawMany(r2.total));\n\n}","folder":null,"sort":0,"permission":{"default":0,"THB0iiaRQeds6BDT":3},"flags":{"advanced-macros":{"runAsGM":false},"core":{"sourceId":"Macro.KM5K7m9zziMBbqSV"},"cf":{"id":"temp_0f7y2lznfe3s","color":"#000000"}},"_id":"zK4vvM02ndLX5f3u"}
{"name":"2d4 750sp Art Objects, Roll 1d4 times on Magic Item Table A and 1d6 times on Magic Item Table B","type":"script","author":"THB0iiaRQeds6BDT","img":"icons/svg/chest.svg","scope":"global","command":"main()\nasync function main() {\n\n  let r1 = new Roll('2d4').roll({async: false});\n  await console.log(r1.total)\n\n  let r2 = new Roll('1d4').roll({async: false});\n  await console.log(r2.total)\n\n  let r3 = new Roll('1d6').roll({async: false});\n  await console.log(r3.total)\n\n//Reading the pack from Compendium\n\nconst pack = game.packs.get(\"cc5e.rolltables-custom\");\n\nawait pack.getIndex();\n\n//Find the right table\n\nlet entry = pack.index.find(e => e.name === \"750sp Art Objects\");\n\n//Rolling on the table\n\npack.getDocument(entry._id).then(table => table.drawMany(r1.total));\n\n//Find the right table\n\nlet entry2 = pack.index.find(e => e.name === \"Magic Item Table A\");\n\n//Rolling on the table\n\npack.getDocument(entry2._id).then(table => table.drawMany(r2.total));\n\n//Find the right table\n\nlet entry3 = pack.index.find(e => e.name === \"Magic Item Table B\");\n\n//Rolling on the table\n\npack.getDocument(entry3._id).then(table => table.drawMany(r3.total));\n\n}","folder":null,"sort":0,"permission":{"default":0,"THB0iiaRQeds6BDT":3},"flags":{"advanced-macros":{"runAsGM":false},"core":{"sourceId":"Macro.KM5K7m9zziMBbqSV"},"cf":{"id":"temp_0f7y2lznfe3s","color":"#000000"}},"_id":"zxlSmuvLiKfkBUod"}
