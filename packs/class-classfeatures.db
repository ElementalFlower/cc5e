{"_id":"q00LlLM5Kg3LAUW5","name":"Gift of the Drowned Ones","type":"feat","img":"icons/svg/item-bag.svg","data":{"description":{"value":"<p>At 3rd level when you adopt this path, you gain a swimming speed equal to your walking speed and gain the ability to breathe underwater.</p>","chat":"","unidentified":""},"source":"Legend of Runeterra: Dark Tides of Bilgewater","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":null},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"requirements":"Path of the Depths 3","recharge":{"value":null,"charged":false}},"effects":[{"_id":"wkTnUiIN8QGeckm4","changes":[{"key":"data.attributes.movement.swim","mode":5,"value":"@attributes.movement.walk","priority":"20"}],"disabled":false,"duration":{"startTime":null},"icon":"icons/svg/aura.svg","label":"Swimming Speed","origin":"Compendium.cc5e.class-classfeatures.q00LlLM5Kg3LAUW5","transfer":true,"flags":{"core":{"statusId":""},"dae":{"stackable":"none","durationExpression":"","specialDuration":[],"transfer":true}},"tint":null,"selectedKey":"data.attributes.movement.swim"}],"folder":null,"sort":0,"permission":{"default":0,"THB0iiaRQeds6BDT":3},"flags":{"betterRolls5e":{"critRange":{"type":"String","value":null},"critDamage":{"type":"String","value":""},"quickDesc":{"type":"Boolean","value":true,"altValue":true},"quickAttack":{"type":"Boolean","value":true,"altValue":true},"quickSave":{"type":"Boolean","value":true,"altValue":true},"quickDamage":{"type":"Array","value":[],"altValue":[],"context":[]},"quickProperties":{"type":"Boolean","value":true,"altValue":true},"quickCharges":{"type":"Boolean","value":{"use":true